extends layouts/main
include mixins/videoBlock

block content
  .search__header
    h3 Search keyword #{searchingBy}
  .search__videos
    if videos.length === 0
      h5 No video files.
    each item in videos
      +videoBlock({
        id: item.id,
        title: item.title,
        views: item.views,
        videoFile:item.fileUrl,
      })
